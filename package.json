{
  "name": "github-dark",
  "title": "GitHub Dark",
  "version": "1.22.131",
  "description": "Dark GitHub style",
  "license": "CC-BY-SA-4.0",
  "repository": "StylishThemes/GitHub-Dark",
  "homepage": "https://github.com/StylishThemes/GitHub-Dark",
  "main": "github-dark.css",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "authors": "bash tools/authors.sh",
    "clean": "npm run perfectionist && node tools/fix-perfectionist.js",
    "generate": "node tools/generate.js",
    "eslint": "eslint --quiet --color *.js tools/*.js",
    "stylelint": "stylelint github-dark.css themes/src/**/*.css",
    "imagemin": "bash tools/imagemin.sh",
    "test": "npm run eslint && npm run stylelint",
    "usercss": "node tools/build-usercss.js",
    "perfectionist": "perfectionist github-dark.css github-dark.css --indentSize 2 --maxAtRuleLength 250",
    "patch": "ver -p -d -C patch github-dark.css github-dark.user.css",
    "minor": "ver -p -d -C minor github-dark.css github-dark.user.css",
    "major": "ver -p -d -C major github-dark.css github-dark.user.css",
    "update": "updates -cu && npm install"
  },
  "devDependencies": {
    "css": "2.2.4",
    "css-mediaquery": "0.1.2",
    "eslint": "6.6.0",
    "eslint-config-silverwind": "5.0.0",
    "grunt": "1.0.4",
    "grunt-contrib-clean": "2.0.0",
    "grunt-contrib-cssmin": "3.0.0",
    "grunt-exec": "3.0.0",
    "grunt-string-replace": "1.3.1",
    "grunt-wrap": "0.3.1",
    "make-fetch-happen": "6.0.1",
    "parse5": "5.1.0",
    "perfectionist": "2.4.0",
    "postcss": "7.0.21",
    "stylelint": "11.1.1",
    "stylelint-config-silverwind": "2.0.4",
    "unzipper": "0.10.5",
    "updates": "9.0.1",
    "url-toolkit": "2.1.6",
    "ver": "6.0.2"
  }
}
